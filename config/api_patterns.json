{
  "gemini": {
    "name": "Google Gemini",
    "pattern": "AIzaSy[a-zA-Z0-9_-]{33}",
    "validation_url": "https://generativelanguage.googleapis.com/v1/models",
    "validation_method": "GET",
    "header_format": "x-goog-api-key: {key}",
    "test_model": "gemini-1.5-flash",
    "search_queries": [
      "AIzaSy in:file",
      "AIzaSy in:file extension:json",
      "AIzaSy in:file filename:.env",
      "gemini api key in:file",
      "GEMINI_API_KEY in:file"
    ],
    "env_vars": ["GEMINI_API_KEY", "GOOGLE_API_KEY"],
    "enabled": true
  },
  "openai": {
    "name": "OpenAI GPT",
    "pattern": "sk-[a-zA-Z0-9]{48}",
    "validation_url": "https://api.openai.com/v1/models",
    "validation_method": "GET",
    "header_format": "Authorization: Bearer {key}",
    "search_queries": [
      "sk- in:file",
      "sk- in:file extension:json",
      "openai api key in:file",
      "OPENAI_API_KEY in:file",
      "gpt api key in:file"
    ],
    "env_vars": ["OPENAI_API_KEY", "OPENAI_KEY"],
    "enabled": false
  },
  "anthropic": {
    "name": "Anthropic Claude",
    "pattern": "sk-ant-api[0-9]{2}-[a-zA-Z0-9_-]{93}",
    "validation_url": "https://api.anthropic.com/v1/messages",
    "validation_method": "POST",
    "header_format": "x-api-key: {key}",
    "search_queries": [
      "sk-ant in:file",
      "sk-ant-api in:file",
      "ANTHROPIC_API_KEY in:file",
      "claude api key in:file"
    ],
    "env_vars": ["ANTHROPIC_API_KEY", "CLAUDE_API_KEY"],
    "enabled": false
  },
  "cohere": {
    "name": "Cohere",
    "pattern": "co-[a-zA-Z0-9]{40}",
    "validation_url": "https://api.cohere.ai/v1/check-api-key",
    "validation_method": "GET",
    "header_format": "Authorization: Bearer {key}",
    "search_queries": [
      "co- in:file",
      "COHERE_API_KEY in:file",
      "cohere key in:file"
    ],
    "env_vars": ["COHERE_API_KEY"],
    "enabled": false
  },
  "huggingface": {
    "name": "Hugging Face",
    "pattern": "hf_[a-zA-Z0-9]{34}",
    "validation_url": "https://huggingface.co/api/whoami",
    "validation_method": "GET",
    "header_format": "Authorization: Bearer {key}",
    "search_queries": [
      "hf_ in:file",
      "HUGGING_FACE_HUB_TOKEN in:file",
      "HF_TOKEN in:file"
    ],
    "env_vars": ["HUGGING_FACE_HUB_TOKEN", "HF_TOKEN"],
    "enabled": false
  },
  "aws": {
    "name": "AWS Access Key",
    "pattern": "AKIA[0-9A-Z]{16}",
    "secret_pattern": "[a-zA-Z0-9/+=]{40}",
    "validation_url": "https://sts.amazonaws.com/",
    "validation_method": "POST",
    "search_queries": [
      "AKIA in:file",
      "AWS_ACCESS_KEY_ID in:file",
      "aws_access_key_id in:file"
    ],
    "env_vars": ["AWS_ACCESS_KEY_ID", "AWS_SECRET_ACCESS_KEY"],
    "enabled": false,
    "note": "AWS需要同时有access key和secret key才能验证"
  },
  "azure": {
    "name": "Azure OpenAI",
    "pattern": "[a-z0-9]{32}",
    "validation_url": "https://{resource}.openai.azure.com/openai/deployments",
    "validation_method": "GET",
    "header_format": "api-key: {key}",
    "search_queries": [
      "AZURE_OPENAI_KEY in:file",
      "AZURE_OPENAI_API_KEY in:file",
      "azure openai key in:file"
    ],
    "env_vars": ["AZURE_OPENAI_KEY", "AZURE_OPENAI_API_KEY"],
    "enabled": false,
    "note": "Azure需要知道resource name才能验证"
  }
}